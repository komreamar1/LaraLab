{
     "compilerOptions": {
      "outDir": "./public/dist/",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "es2022",
      "target": "ES6",
      "jsx": "react",
      "lib": ["DOM", "ES6", "DOM.Iterable", "ScriptHost", "ES2016.Array.Include"],
      "allowJs": true,
      "moduleResolution": "node",
  },
    "include": ["resources/src/**/*.tsx", "resources/src/**/*.ts", "resources/js/**/*.d.ts", "resources/js/**/*.vue"]
}